<div class="wsd-page">
    <div class="wsd-page__header">
        <a class="wsd-page__header-logo">
            <img src="{{rootURL}}assets/images/sysdig-white-logo.png">
        </a>
        <div class="wsd-page__header-title">
            <span>{{captureFilePath}}</span>
        </div>
        <ul class="wsd-page__header-menu">
            <li class="wsd-page__header-menu-option">
                <a class="wsd-page__header-menu-button" {{action "openFile"}}>
                    {{svg-jar "file_white" class="icon" width="24px" height="24px"}} Open
                </a>
            </li>
        </ul>
    </div>

    <div class="wsd-page__content">
        {{outlet}}
    </div>
</div>
