<div class="metric-summary__header">
    <div class="metric-summary__header-title">{{data.name}}</div>
</div>

<div class="metric-summary__value">
    {{number-to-string data.totalValue}}
</div>

<div class="metric-summary__time-series">
    {{sd-time-series
        height=timeSeriesHeight
        width=timeSeriesWidth
        timestampProp="t"
        valueProp="v"
        data=data.timeSeries
    }}
</div>
