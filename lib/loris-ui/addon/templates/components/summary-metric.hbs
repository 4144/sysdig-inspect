<div class="summary-metric__header">
    <div class="summary-metric__header-title">{{data.name}}</div>
</div>

<div class="summary-metric__value">
    {{number-to-string data.totalValue}}
</div>

<div class="summary-metric__time-series">
    {{sd-time-series
        height=timeSeriesHeight
        width=timeSeriesWidth
        timestampProp="t"
        valueProp="v"
        data=data.timeSeries
    }}
</div>